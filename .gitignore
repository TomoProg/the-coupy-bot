# Ignore bundler config.
/web/app/.bundle

# Ignore all logfiles and tempfiles.
/web/app/log/*
/web/app/tmp/*
/web/app/public/assets/*
!/web/app/log/.keep
!/web/app/tmp/.keep

/web/app/node_modules
/web/app/yarn-error.log

.byebug_history

